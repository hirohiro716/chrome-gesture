{
    "name": "hirohiro716 Gesture",
    "manifest_version": 2,
    "version": "1.0",
    "description": "Mouse gesture of hirohiro716.",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com/; object-src 'self'",
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*" ],
            "js": ["jquery.min.js", "gesture.js"]
        }
    ],
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}

